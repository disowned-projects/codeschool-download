{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "indent": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "comma-dangle": "off",
        "prefer-template": "off",
        "wrap-iife": "off",
        "no-trailing-spaces": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-return-assign": "off",
        "no-shadow": "off",
        "no-param-reassign": "off"
    },
    "globals": {
        "$": true
    },
    "env": {
        "jest": true
    }
}